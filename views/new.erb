<form method="post" action="/hogwarts-tracker/new">
  <label for="first-name-feild">First Name:</label>
  <input
  type="text"
  name="first_name"
  id="first-name-field"
  />

  <label for="second-name-field">Last Name:</label>
  <input
  type="text"
  name="second_name"
  id="second-name-field"
  />

  <label for="house-selection">Select a house:</label>
  <select name="house_id" id="house-selection">
    <option value="1">Gryffindor</option>
    <option value="2">Ravenclave</option>
    <option value="3">Hufflepuff</option>
    <option value="4">Slytherin</option>
  </select>

  <label for="age-field">Age:</label>
  <input
  type="number"
  name="age"
  id="age-field"
  />

  <input type="submit" value="Add student">
</form>
